+         e = binop(Iop_F32toI64S, rm, getFReg32(fj));
+         break;
+      case FTINTRNE_L_D:
+         rm = gen_round_to_nearest();
+         e = binop(Iop_F64toI64S, rm, getFReg64(fj));
+         break;
+      case FTINT_L_S:
+         rm = get_rounding_mode();
+         e = binop(Iop_F32toI64S, rm, getFReg32(fj));

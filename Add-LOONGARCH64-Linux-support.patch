+      int res;                                \
+      unsigned long v1 = (unsigned long)val1; \
+      unsigned long v2 = (unsigned long)val2; \
+      __asm__ __volatile__(                   \
+             insn " %1, %2, 1f   \n\t"        \
+         "   move %0, $zero      \n\t"        \
+         "   b 2f                \n\t"        \
+         "1:                     \n\t"        \

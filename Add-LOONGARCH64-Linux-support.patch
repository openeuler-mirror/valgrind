    return success;
 }
 
+#elif defined(VGA_loongarch64)
+
+// loongarch64
+/* return 1 if success, 0 if failure */
+UWord do_acasW ( UWord* addr, UWord expected, UWord nyu )
+{
+   UWord success;
+   UWord block[3] = { (UWord)addr, nyu, expected };
+
+   __asm__ __volatile__(
+      "   ld.d $t0, %1, 0   \n\t"
+      "   ld.d $t2, %1, 16  \n\t"
+      "   ld.d $t3, %1, 8   \n\t"
+      "   ll.d $t1, $t0, 0  \n\t"
+      "   bne  $t1, $t2, 1f \n\t"
+      "   sc.d $t3, $t0, 0  \n\t"
+      "   move %0, $t3      \n\t"
+      "   b    2f           \n\t"
+      "1:                   \n\t"
+      "   move %0, $zero    \n\t"
+      "2:                   \n\t"
+      : /*out*/ "=r" (success)
+      : /*in*/ "r" (&block[0])
+      : /*trash*/ "t0", "t1", "t2", "t3", "memory"
+   );
+
+   assert(success == 0 || success == 1);
+   return success;
+}
+
 #endif
 

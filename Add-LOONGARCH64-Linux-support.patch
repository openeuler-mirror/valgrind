+         break;
+      case FTINTRZ_L_S:
+         rm = gen_round_to_zero();
+         e = binop(Iop_F32toI64S, rm, getFReg32(fj));
+         break;
+      case FTINTRZ_L_D:
+         rm = gen_round_to_zero();
+         e = binop(Iop_F64toI64S, rm, getFReg64(fj));
+         break;
+      case FTINTRNE_L_S:

+      __asm__ __volatile__(                       \
+         "move " rd ", %2      \n\t"              \
+         "move " rj ", %3      \n\t"              \
+         insn " " rd ", " rj " \n\t"              \
+         "move %0, " rd "      \n\t"              \
+         "move %1, " rj "      \n\t"              \
+         : "=r" (res1), "=r" (res2)               \
+         : "r" (val1), "r" (val2)                 \
+         : rd, rj, "memory");                     \
+      printf("%s %s, %s ::\n", insn, rd, rj);     \
+      printf("before: %s=%#018lx, %s=%#018lx\n",  \
+             rd, val1, rj, val2);                 \
+      printf("after:  %s=%#018lx, %s=%#018lx\n",  \
+             rd, res1, rj, res2);                 \
+   }

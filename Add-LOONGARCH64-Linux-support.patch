+/* This is the loongarch64 variant of memcheck/tests/x86-linux/scalar.c.
+   Syscalls are in x86 number order to make exp comparison easier. */
+#define _GNU_SOURCE
+#include "../../memcheck.h"
+#include "scalar.h"
+#include <unistd.h>
